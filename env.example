# 应用配置
APP_NAME=Navigator Demo
APP_VERSION=0.1.0
NODE_ENV=development

# 开发服务器配置
VITE_DEV_PORT=3000
VITE_PREVIEW_PORT=4173

# Electron配置
ELECTRON_IS_DEV=true

# PDF处理配置
VITE_PDF_WORKER_SRC=/pdf.worker.min.js
VITE_PDF_CMAP_URL=https://unpkg.com/pdfjs-dist@3.11.174/cmaps/
VITE_PDF_STANDARD_FONT_DATA_URL=https://unpkg.com/pdfjs-dist@3.11.174/standard_fonts/

# 日志配置
VITE_LOG_LEVEL=debug

# 功能开关
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_AUTO_UPDATE=false 